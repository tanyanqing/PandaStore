<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2dip" />
            <gradient
                android:angle="0"
                android:centerColor="#00a7a7a7"
                android:centerY="0.75"
                android:endColor="#00a7a7a7"
                android:startColor="#00a7a7a7" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="0"
                    android:centerColor="#b066dec3"
                    android:endColor="#8066dec3"
                    android:startColor="#ff66dec3" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:angle="0"
                    android:centerColor="#b066dec3"
                    android:endColor="#8066dec3"
                    android:startColor="#ff66dec3" />
            </shape>
        </clip>
    </item>
</layer-list>